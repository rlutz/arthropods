pad-length = 50mil

line-thickness = 10mil

pin-thickness = 1.5mm
pin-clearance = 1.4mm
pin-mask = 1.7mm
pin-drill = 1mm

pad-thickness = 1.492mm
pad-clearance = 1.4mm
pad-mask = 1.69mm

output-file = footprints/$(name).fp

[variety "SIP"]
	row-count = 1
	style = thickened
	name = SIP$(total-pin-count)

